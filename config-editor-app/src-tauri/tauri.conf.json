{
  "productName": "Audio Notifier Config",
  "version": "1.0.0",
  "identifier": "com.cwveysey.audio-notifier-config",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "",
    "longDescription": "",
    "macOS": {
      "minimumSystemVersion": "10.15"
    }
  },
  "app": {
    "windows": [
      {
        "title": "Coding agent audio notifications",
        "width": 900,
        "height": 950,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 700,
        "minHeight": 600,
        "center": true,
        "decorations": true,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  }
}
